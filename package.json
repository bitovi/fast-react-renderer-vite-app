{
  "name": "fast-react-static-renderer-vite-app",
  "author": "Bitovi",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint src",
    "depcheck": "depcheck .",
    "prettier": "prettier --check src",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@bitovi/eslint-config/react"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.16.5",
    "@contentful/rich-text-types": "^16.2.0",
    "lazysizes": "^5.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.11.2",
    "react-router-dom": "^6.11.2"
  },
  "devDependencies": {
    "@bitovi/eslint-config": "^1.5.1",
    "@types/node": "^20.2.5",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.38.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
